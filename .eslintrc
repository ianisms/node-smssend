{
    "env": { 
        "es6": true, 
        "node": true 
    },
    "extends": "google",
    "parserOptions": { "ecmaVersion": 6 },
    "plugins": ["node"],
    "rules": {
        "linebreak-style": 0,
        "require-jsdoc": 0,
        "max-len": ["error", { "ignoreUrls": true, "ignoreStrings": true }]
    }
}